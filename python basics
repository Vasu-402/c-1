        "def listSum(numbers):\n",
        "  if not numbers:\n",
        "    return 0\n",
        "  else:\n",
        "    (f, rest) = numbers\n",
        "    return f + listSum(rest)\n",
        "\n",
        "myList = (1, (2, (3, None)))\n",
        "total = listSum(myList)\n",
        "print(total)"
      ],
